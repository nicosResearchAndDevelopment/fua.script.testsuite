tsm:timestamp_Shape
    a                   sh:NodeShape ;
    sh:targetSubjectsOf tsm:timestamp ;
    sh:property         [ a           sh:PropertyShape ;
                          sh:path     tsm:timestamp ;
                          sh:maxCount 1 ;
                          sh:datatype xsd:dateTimeStamp ; ] ;
.

tsm:Applicant_Shape
    a              sh:NodeShape ;
    sh:targetClass tsm:Applicant ;
.

tsm:Entry_Shape
    a              sh:NodeShape ;
    sh:targetClass tsm:Entry ;
    sh:property    [ a           sh:PropertyShape ;
                     sh:path     tsm:key ;
                     sh:minCount 1 ;
                     sh:maxCount 1 ;
                     sh:datatype xsd:string ; ] ;
    sh:property    [ a           sh:PropertyShape ;
                     sh:path     tsm:value ;
                     sh:minCount 1 ;
                     sh:maxCount 1 ; ] ;
    sh:property    [ a           sh:PropertyShape ;
                     sh:path     tsm:timestamp ;
                     sh:minCount 1 ;
                     sh:maxCount 1 ;
                     sh:datatype xsd:dateTimeStamp ; ] ;
.
